{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "watchAssets": true,
    "assets": [
      "**/*.hbs",
      "**/*.json",
      "**/*.yaml",
      "**/*.yml"
    ]
  },
  "projects": {
    "rwalandchain-backend": {
      "type": "application",
      "root": "",
      "entryFile": "main",
      "sourceRoot": "src",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.json"
      }
    }
  },
  "monorepo": false,
  "root": ".",
  "generateOptions": {
    "spec": true,
    "flat": false
  }
}

