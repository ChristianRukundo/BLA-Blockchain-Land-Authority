{
  "name": "@rwalandchain/backend",
  "version": "2.0.0",
  "description": "RwaLandChain Backend API Server",
  "author": "RwaLandChain Team",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate -d src/database/data-source.ts",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- migration:run -d src/database/data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/database/data-source.ts",
    "migration:show": "npm run typeorm -- migration:show -d src/database/data-source.ts",
    "schema:sync": "npm run typeorm -- schema:sync -d src/database/data-source.ts",
    "schema:drop": "npm run typeorm -- schema:drop -d src/database/data-source.ts",
    "seed": "ts-node -r tsconfig-paths/register src/database/seeds/run-seeds.ts",
    "db:reset": "npm run schema:drop && npm run migration:run && npm run seed",
    "test:setup": "./scripts/test-setup.sh",
    "test:api": "./scripts/test-api.sh",
    "dev:start": "./scripts/start-dev.sh",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist node_modules"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.8",
    "@nestjs/core": "^10.2.8",
    "@nestjs/platform-express": "^10.2.8",
    "@nestjs/typeorm": "^10.0.1",
    "@nestjs/config": "^3.1.1",
    "@nestjs/swagger": "^7.1.16",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/cache-manager": "^2.1.1",
    "@nestjs/throttler": "^5.0.1",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/terminus": "^10.2.0",
    "typeorm": "^0.3.17",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "cache-manager": "^5.2.4",
    "cache-manager-redis-store": "^3.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.1",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.5",
    "winston": "^3.11.0",
    "nest-winston": "^1.9.4",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1",
    "joi": "^17.11.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "ethers": "^6.8.1",
    "axios": "^1.6.2",
    "nodemailer": "^6.9.7",
    "handlebars": "^4.7.8",
    "moment": "^2.29.4",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.8",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/supertest": "^2.0.16",
    "@types/bcryptjs": "^2.4.6",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^9.0.7",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/nodemailer": "^6.4.14",
    "@types/lodash": "^4.14.202",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "rimraf": "^5.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
